<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sg.mapper.CarShopMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sg.bean.CarShop">
        <id column="ID" property="id" />
        <result column="USER_ID" property="userId" />
        <result column="USER_NAME" property="userName" />
        <result column="PHONE" property="phone" />
        <result column="SHOP_NAME" property="shopName" />
        <result column="PROVINCE" property="province" />
        <result column="CITY" property="city" />
        <result column="AREA" property="area" />
        <result column="ADDRESS" property="address" />
        <result column="STATUS" property="status" />
        <result column="CREATER" property="creater" />
        <result column="CREATEDATE" property="createdate" />
        <result column="UPDATER" property="updater" />
        <result column="UPDATEDATE" property="updatedate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, USER_ID, USER_NAME, PHONE, SHOP_NAME, PROVINCE, CITY, AREA, ADDRESS, STATUS, CREATER, CREATEDATE, UPDATER, UPDATEDATE
    </sql>

</mapper>
